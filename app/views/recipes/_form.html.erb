<%= form_for @recipe do |f| %>
  Title: <%=f.text_field :title%>
  <%binding.pry%>

  <%= f.fields_for :ingredients do |ingredient| %>
  <%= ingredient.label :ingredient_1 %>
  <%= ingredient.text_field :ingredient_1 %>

  <%= ingredient.label :ingredient_2 %>
  <%= ingredient.text_field :ingredient_2 %>

  <%= ingredient.label :name %>
  <%= ingredient.text_field :name %>

  <%= ingredient.label :quantity %>
  <%= ingredient.text_field :quantity %>

  <%end%>
  <%=f.submit%>
<% end %>
